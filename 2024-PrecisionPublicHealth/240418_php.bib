@article{ WOS:000457554400038,
Author = {Alshehri, Abdulrahim Abdullah and Ambrosino, Elena},
Title = {OPPORTUNITIES FOR PRECISION MEDICINE AND PRECISION PUBLIC HEALTH IN
   SAUDI ARABIA},
Journal = {INDO AMERICAN JOURNAL OF PHARMACEUTICAL SCIENCES},
Year = {2019},
Volume = {6},
Number = {1},
Pages = {2118-2127},
Month = {JAN},
Abstract = {Precision approaches in health stem from innovations in basic sciences
   and - omic technologies and rely on individuals' genomic structure to
   develop tailored treatment and prevention opportunities. Saudi Arabia
   has recently shown increasing interest in implementing precision
   approaches to improve healthcare and tackle its major health challenges.
   This study investigated opportunities, requirements and barriers in the
   implementation of precision approaches in health in Saudi Arabia.
   A narrative literature review included resources published in English
   and Arabic after 1995 if identified by search terms.
   Opportunities for implementation of precision approaches were identified
   in the context of the main local health issues. Requirements for
   precision medicine implementation were identified, including the ability
   to collect and process data. In addition, barriers and challenges, as
   lack of awareness of the field amongst health staff, were pointed out.
   The outcome offers relevant information to implementation attempts in
   the field and may help guiding policy-making efforts.},
