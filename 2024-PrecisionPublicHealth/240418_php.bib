@article{ WOS:000718721400003,
Author = {Sausen, Kathleen A. and Randolph, James W. and Casciato, Allison N. and
   Dietrich, Mary S. and Scholer, Seth J.},
Title = {The Development, Preliminary Validation, and Clinical Application of the
   Quick Parenting Assessment},
Journal = {PREVENTION SCIENCE},
Year = {2022},
Volume = {23},
Number = {2},
Pages = {306-320},
Month = {FEB},
Abstract = {To prevent diseases, efforts are needed to determine how to address
   Adverse Childhood Experiences (ACEs), including parenting behaviors. The
   objective of this study, conducted in Nashville TN in 2017, was to
   initiate testing the psychometric properties of two new Adverse
   Childhood Experiences (ACEs) screening tools, the Quick Parenting
   Assessment (QPA) and Other Childhood Stressors (OCS). In a clinic
   serving low-income families, caregivers of children ages 2-10 completed
   assessments of parenting (QPA), other stressors (OCS), child behavior
   problems ((Strength and Difficulties Questionnaire (SDQ)), and Attitudes
   Toward Spanking (ATS). The QPA takes 1 min to complete and assesses for
   healthy and unhealthy parenting behaviors. Seventy-five percent of
   eligible participants completed the survey (N=558). A reduced 10-item
   QPA yielded a Cronbach's alpha of 0.79 and, in 4-10-year-olds, was
   associated with high SDQ conduct, hyperactivity, and total difficulties
   scores (r=0.44, 0.48, and 0.47; all p< 0.001). Children with QPAs of >4
   were nine times more likely than those children with scores of <= 2 to
   have behavior problems (OR=8.93, 95\% CI = 3.74-21.32). Elevated QPAs
   were associated with the ATS (r=0.47, p < .001). The OCS was also
