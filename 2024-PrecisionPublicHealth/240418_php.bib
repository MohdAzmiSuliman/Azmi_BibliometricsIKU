@article{ WOS:000441781500012,
Author = {Ollier, William and Muir, Kenneth R. and Lophatananon, Artitaya and
   Verma, Arpana and Yuille, Martin},
Title = {Risk biomarkers enable precision in public health},
Journal = {PERSONALIZED MEDICINE},
Year = {2018},
Volume = {15},
Number = {4},
Pages = {329-342},
Month = {JUL},
Abstract = {Precision medicine uses biomarkers to diagnose disease. However, they
   can also be used to measure risk of disease. Thus, biomarkers herald a
   new addition to public health - Precision Public Health. We examine the
   implications. Risk biomarkers are identified by analyzing population
   cohorts. They constitute risk factors in mathematical Disease Risk
   Models'. The risk may be fixed as in a genetic biomarker or variable as
   in some protein biomarkers. They help monitor current risk of disease in
   an individual, thereby aiding efforts to reduce risk. In the UK, the NHS
   Health Check system is a universal system for assessing risk and for
   risk reduction. The system can now make use of modern biomarkers once
   appropriate infrastructure and governance are in place.},
